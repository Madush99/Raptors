<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shoppingapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_size_invert"
        android:label="WTF_Helinsa"
        android:roundIcon="@drawable/logo_size_invert"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".startup"></activity>
        <activity android:name=".login" />
        <activity android:name=".signup" />
        <activity android:name=".PicassoExample">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddClothes" />
        <activity android:name=".addCard" />
        <activity android:name=".ClothesCategory" />
    </application>

</manifest>